{
  "root": true,
  "plugins": [
    "@typescript-eslint",
    "prettier",
    "unicorn",
    "import"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    // "airbnb-typescript/base",
    // "plugin:@typescript-eslint/recommended",
    "plugin:unicorn/recommended"
    // "plugin:prettier/recommended",
    // "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "project": "./tsconfig.json",
    "sourceType": "module"
  },
  "globals": {
    "cy": "readonly"
  },
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "unicorn/no-null": "off"
  }
}
